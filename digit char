
#include <stdio.h>
#include<string.h>
int main()
{
    char str1[50];
    scanf("%[^\n]s",str1);
    int i,j,n;
    n=strlen(str1);
    for(i=1;i<n;i=i+2)
    {
        int r=(int)str1[i]-48;
        for(j=1;j<=r;j++)
       
        {
            
            printf("%c",str1[i-j]);
        }
    }

    return 0;
}
